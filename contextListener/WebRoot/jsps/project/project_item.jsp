			$noAmountTips.hide();
		}
		var incomeMoney = numberFormat(repayPlan(money, total_rate, sTime, eTime), 2);
		$income.text(incomeMoney);
	}
	if($('.status-buy').length){
		countIncome();
	}
	$amount.on('keyup', countIncome).on('blur', countIncome).focus();
	
