				delegate: 'a.withimg',
				type: 'image',
				gallery:{
					enabled:true
				}
			});
		});
	}
	//斗牛行动
	if (is_douniu) {
		$('.info-details a').eq(0).removeClass('withimg').prop('href', window.CONFIG.BASE_URI + '/douniuxingdong').prop('target', '_blank');
