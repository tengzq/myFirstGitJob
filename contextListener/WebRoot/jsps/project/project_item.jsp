		var value = $.cookie.get(cookie_key);
		if(value && value.length!==0){
			$amount.val(value);
		}
	}
	
	setAmount();

	function countIncome(){
		$amount.val($amount.val().replace(/[^\d]/g, ''));
		var money = parseInt($amount.val() || 0, 10);
		if(!money){
			$noAmountTips.show();
			money = 100000;
